{
  "evilginx": {
    "enabled": true,
    "url": "http://evilginx2:80",
    "use_for_urls": true,
    "phishlet_config_dir": "/opt/evilginx/phishlets",
    "default_phishlet": "linkedin",
    "dns_port": 53,
    "http_port": 80,
    "https_port": 443
  },
  "gophish": {
    "admin_server": {
      "listen_url": "0.0.0.0:3333",
      "use_tls": false,
      "cert_path": "gophish_admin.crt",
      "key_path": "gophish_admin.key"
    },
    "phish_server": {
      "listen_url": "0.0.0.0:8080",
      "use_tls": false,
      "cert_path": "example.crt",
      "key_path": "example.key"
    },
    "database_name": "sqlite3",
    "database_path": "gophish.db"
  },
  "network": {
    "hack_network": "hack_network"
  },
  "logging": {
    "level": "info",
    "file": "/opt/gophish/data/integration.log"
  }
}
